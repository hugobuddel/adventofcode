number = { ASCII_DIGIT }
term = { number | "(" ~ expression ~ ")"}
operator = {"+" | "-"}
action = {" " ~ operator ~ " " ~ product}
actions = {action*}
product = {(number | "(" ~ expression ~ ")") ~ (" * " ~ (number | "(" ~ expression ~ ")"))*}
expression = { product ~ actions }
calculator = { (expression ~ "\n")+ }
file = {SOI ~ calculator ~ EOI}
