name = {ASCII_DIGIT+}
andterms = {name ~ (" " ~ name)*}
orterms = {andterms ~ (" | " ~ andterms)*}
letter = {"\"" ~ ASCII_ALPHA ~ "\""}
rule = {name ~ ": " ~ (orterms | letter)}
rules = {(rule ~ "\n")+}
monster = {ASCII_ALPHA+}
monsters = {(monster ~ "\n")+}
file = { SOI ~ rules ~ "\n" ~ monsters ~ EOI}
